@model DestCovery.Models.User_Info
@{
    ViewBag.Title = "DestCovery - Login";
    Layout = "~/Views/Shared/_Layout_login.cshtml";
}

<div class="wrap">
    <h2>Login </h2>

    @using (Html.BeginForm("User_Registraion_Form", "Home", FormMethod.Post, new { enctype = "multipart/form-data" }))
    {

        <div class="form-group">
            <label>Email Id :</label>@Html.TextBoxFor(x => x.User_Id, new { @class = "logintexbox" })
        </div>

        <div class="form-group">
            <label>Password:</label>@Html.PasswordFor(x => x.User_Password, new { @class = "logintexbox" })
        </div>

        <div class="text-center">
            <input type="submit" value="Submit" class="loginsubmit" />
        </div>

    }



</div>

