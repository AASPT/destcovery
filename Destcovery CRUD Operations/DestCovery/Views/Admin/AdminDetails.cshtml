@model IEnumerable<DestCovery.Models.Admin_Info>
@{
    ViewBag.Title = "AdminDetails";
    Layout = "~/Views/Shared/_Layout_admin_dashboard.cshtml";
}

<h2>AdminDetails</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr class="btn-primary">
        <th>
            @Html.DisplayNameFor(model => model.Admin_Id)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Admin_Name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Admin_Email)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Admin_Password)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Admin_Mobile)
        </th>
        <th>Action</th>
    </tr>

    @foreach (var item in Model)
    {
      
    <tr class="btn-info">

    @*@Html.AntiForgeryToken()*@
    <td>
        @Html.DisplayFor(modelItem => item.Admin_Id)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.Admin_Name)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.Admin_Email)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.Admin_Password)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.Admin_Mobile)
    </td>
    <td>
        @Html.ActionLink("Edit", "Edit", new { id = item.Admin_Id }) |
        @Html.ActionLink("Details", "Details", new { id = item.Admin_Id }) |
        @Html.ActionLink("Delete", "Delete", new { id = item.Admin_Id })
    </td>
</tr>
            

        }

    </table>  